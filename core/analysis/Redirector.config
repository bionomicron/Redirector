# Author Graham Rockwell
# Suggested defaults and example configurations for Redirector optimization framework
# Command line example
# >python Redirector.py -n 'Default' -m "iAF1260,iAF1260_Production" -b Biomass -s EX_kdg --iter 10 --sn 1 --report -v --section kdg

# Current Issues: Last update 2013.04.16
# Simple model using flat control not saving real optimal final solution.

#-------------------------------------------
# Base line setting for all configurations
#-------------------------------------------
[Redirector Model]

#---directories---
dataDirectory: ../../data
libraryDirectory: ControlLibraries/
resultDirectory: ../../results/
analysisDirectory: ControlSearches

#---model config file---
modelConfigFile: model.config

#---solver---
solver= PuLP_SCIP
solverConfig: ''
resultTag = cp
useControlPenalty=True
orderGenes= True
iterReport = True

#---MILP optimization---
#primeBounds: False
posControlBound = 100
negControlBound = -100
posFluxControlBound = 100
negFluxControlBound = -100
objectiveCoeffecent = 1.0

#---control search---
controlMin = 0
delta=1e-5
coeffecentDefault = 0.0

#----------------------------------
# Suggested default settings
#----------------------------------
[Default]

#---display---
debug: False
verbose: True 

#---optimization settings---
bioTarget: 0.2 
naturalScale: 0.2
#aneal: 0
#preStart: 0

#---control library---
useGeneMap: True
#orderGenes: True
#simoControl: 1
#control: flat
searchIterMax = 15

#---report---
iterReport: True
#isReport: True

#------------------------------------
# Testing of a Simple Model
